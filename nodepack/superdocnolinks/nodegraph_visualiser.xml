&lt;script type=&quot;text/javascript&quot; language=&quot;javascript&quot;&gt; 
&lt;!--
var traverseNode = function (startFresh) {
  if ( document.applets &amp;&amp; document.applets.length &gt; 0 ) {
    var papplet = document.applets[0];
    if ( papplet.frameCount &gt; 0 ) {
      var seedNode = document.nodegraphControls.seedNode.value;
      var breadth = document.nodegraphControls.breadth.value;
      var depth = document.nodegraphControls.depth.value;
      if (!startFresh) {
        papplet.traverseTree(seedNode, breadth, depth);
      }
      else {
        papplet.traverseNewTree(seedNode, breadth, depth);
      }
    }
  }
  return true;
}
//--&gt;&lt;/script&gt;


